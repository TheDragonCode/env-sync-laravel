{
    "name": "dragon-code/env-sync-laravel",
    "description": "env-sync package adapter for Laravel framework",
    "type": "library",
    "license": "MIT",
    "keywords": ["env", "dotenv", "environment", "sync", "laravel", "framework", "dragon-code", "dragon", "andrey-helldar"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/env-sync-laravel/issues",
        "source": "https://github.com/TheDragonCode/env-sync-laravel"
    },
    "require": {
        "php": "^7.3|^8.0",
        "dragon-code/env-sync": "^3.0"
    },
    "require-dev": {
        "dragon-code/support": "^5.2",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DragonCode\\EnvSync\\Frameworks\\Laravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "DragonCode\\EnvSync\\Frameworks\\Laravel\\ServiceProvider"
            ]
        }
    }
}
